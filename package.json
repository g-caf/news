{
  "name": "rss-aggregator-backend",
  "version": "1.0.0",
  "description": "RSS aggregation backend with feed parsing and article management",
  "main": "src/server.js",
  "scripts": {
    "start": "npm run migrate && node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node src/migrations/migrate.js",
    "seed": "node src/seeds/seed.js",
    "build": "npm ci --only=production",
    "test": "jest",
    "worker": "node src/workers/rss-worker.js",
    "cron": "node src/jobs/parse-rss.js",
    "postinstall": "npm run migrate"
  },
  "keywords": ["rss", "aggregator", "backend", "express", "postgresql"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "rss-parser": "^3.13.0",
    "node-cron": "^3.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
